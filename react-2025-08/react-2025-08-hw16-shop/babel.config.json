{
	"presets": [
		["@babel/preset-env", { "useBuiltIns": "entry", "corejs": "3.22" }],
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["./src"],
                "alias": {
                    "src": "./src"
                }
            }
        ]
    ]
}
